/* myMsg */
.bg {
  width: 100%;
  position: absolute;
  top: 0;
  right: 0;
  opacity: 0.05;
}
.myMsg {
  width: 90%;
  margin: 46rpx auto 0;
  position: relative;
  z-index: 999;
}
.myMsg image {
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%;
}
.cardAccess, .cardCollection {
  width: 200rpx;
  text-align: center;
  font-size: 14px;
  margin: 46rpx 6rpx 0;
}
.msgTop {
  display: flex;
  justify-content: center;
}
/* msgBottm */
.msgBottom {
  width: 96%;
  background: #fff;
  padding: 46rpx 18rpx 24rpx 18rpx;
  position: absolute;
  top: 148rpx;
  z-index: -1;
  border-radius: 8rpx;
  margin: 0 auto;
}
.msgBottomName {
  font-size: 16px;
  line-height: 1.6;
  border-bottom: 1px solid #ddd;
  padding: 0 0 8rpx 0;
  margin: 0 0 8rpx 0;
}
.msgBottomName label, .msgBottomName text {
  font-size: 12px;
}
.msgBottomName text {
  float: right;
}
.msgBottomItem {
  font-size: 14px;
  line-height: 2;
}
/* btn */
.msgBtn {
  display: flex;
  justify-content: center;
}
.msgBtn label {
  font-size: 12px;
  padding: 8rpx 16rpx;
  background: #169bd5;
  color: #fff;
  margin: 28rpx 16rpx 16rpx;
  display: inline-block;
  width: 140rpx;
  text-align: center;
  border-radius: 6rpx;
}
/* position:absolute消除 */
.clear {
  height: 460rpx;
}
/* companyDis */
.companyDis {
  margin: 0 auto 24rpx;
  background: #fff;
  border-radius: 8rpx;
  padding: 24rpx 22rpx 32rpx;
}
.album {
  margin: 0 auto 0;
}
.companyDis label {
  font-size: 16px;
  font-weight: 600;
  border-left: 10rpx solid #169bd5;
  padding: 0 0 0 12rpx;
}
.line {
  width: 90%;
  height: 1px;
  background: #999;
  display: block;
  margin: 12rpx 0 0 0;
}
.companyContent {
  font-size: 12px;
  width: 96%;
  display: block;
  margin: 12rpx auto 0;
  line-height: 1.8;
}
/* 相册 */
.imgItem {
  display: flex;
}
.height1 {
  width: 100%;
  height: 750rpx;
}
.height2 {
  width: 100%;
  height: 370rpx;
}
.height3 {
  height: 220rpx;
  width: 100%;
}